ontend/src/chat/chat.js",["114"],"import \"./chat.scss\";\nimport { to_Decrypt, to_Encrypt } from \"../aes.js\";\nimport { process } from \"../store/action/index\";\nimport React, { useState, useEffect, useRef } from \"react\";\nimport { useDispatch } from \"react-redux\";\n//gets the data from the action object and reducers defined earlier\nfunction Chat({ username, roomname, socket }) {\n  const [text, setText] = useState(\"\");\n  const [messages, setMessages] = useState([]);\n\n  const dispatch = useDispatch();\n  \n  const dispatchProcess = (encrypt, msg, cipher) => {\n    dispatch(process(encrypt, msg, cipher));\n  };\n\n  useEffect(() => {\n    socket.on(\"message\", (data) => {\n      //decypt the message\n      const ans = to_Decrypt(data.text, data.username);\n      dispatchProcess(false, ans, data.text);\n      console.log(ans);\n      let temp = messages;\n      temp.push({\n        userId: data.userId,\n        username: data.username,\n        text: ans,\n      });\n      setMessages([...temp]);\n    });\n  }, [socket]);\n\n  const sendData = () => {\n    if (text !== \"\") {\n      //encrypt the message here\n      const ans = to_Encrypt(text);\n      socket.emit(\"chat\", ans);\n      setText(\"\");\n    }\n  };\n  const messagesEndRef = useRef(null);\n\n  const scrollToBottom = () => {\n    messagesEndRef.current.scrollIntoView({ behavior: \"smooth\" });\n  };\n\n  useEffect(scrollToBottom, [messages]);\n\n  console.log(messages, \"mess\");\n\n  return (\n    <div className=\"chat\">\n      <div className=\"user-name\">\n        <h2>\n          {username} <span style={{ fontSize: \"0.7rem\" }}>in {roomname}</span>\n        </h2>\n      </div>\n      <div className=\"chat-message\">\n        {messages.map((i) => {\n          if (i.username === username) {\n            return (\n              <div className=\"message\">\n                <p>{i.text}</p>\n                <span>{i.username}</span>\n              </div>\n            );\n          } else {\n            return (\n              <div className=\"message mess-right\">\n                <p>{i.text} </p>\n                <span>{i.username}</span>\n              </div>\n            );\n          }\n        })}\n        <div ref={messagesEndRef} />\n      </div>\n      <div className=\"send\">\n        <input\n          placeholder=\"enter your message\"\n          value={text}\n          onChange={(e) => setText(e.target.value)}\n          onKeyPress={(e) => {\n            if (e.key === \"Enter\") {\n              sendData();\n            }\n          }}\n        ></input>\n        <button onClick={sendData}>Send</button>\n      </div>\n    </div>\n  );\n}\nexport default Chat;\n","/home/dhairya/Desktop/Scribble/chatfrontend/src/process/process.js",[],"/home/dhairya/Desktop/Scribble/chatfrontend/src/home/home.js",[],"/home/dhairya/Desktop/Scribble/chatfrontend/src/store/reducer/process.js",[],"/home/dhairya/Desktop/Scribble/chatfrontend/src/aes.js",[],"/home/dhairya/Desktop/Scribble/chatfrontend/src/store/action/index.js",[],"/home/dhairya/Desktop/sem3/Scribble/chatfrontend/src/index.js",[],["115","116"],"/home/dhairya/Desktop/sem3/Scribble/chatfrontend/src/App.js",[],"/home/dhairya/Desktop/sem3/Scribble/chatfrontend/src/store/reducer/index.js",[],"/home/dhairya/Desktop/sem3/Scribble/chatfrontend/src/process/process.js",["117"],"/home/dhairya/Desktop/sem3/Scribble/chatfrontend/src/home/home.js",[],"/home/dhairya/Desktop/sem3/Scribble/chatfrontend/src/chat/chat.js",["118"],"/home/dhairya/Desktop/sem3/Scribble/chatfrontend/src/store/reducer/process.js",[],"/home/dhairya/Desktop/sem3/Scribble/chatfrontend/src/aes.js",[],"/home/dhairya/Desktop/sem3/Scribble/chatfrontend/src/store/action/index.js",[],"/home/dhairya/Desktop/Scribble_latest/chatfrontend/src/index.js",[],"/home/dhairya/Desktop/Scribble_latest/chatfrontend/src/App.js",[],"/home/dhairya/Desktop/Scribble_latest/chatfrontend/src/store/reducer/index.js",[],"/home/dhairya/Desktop/Scribble_latest/chatfront