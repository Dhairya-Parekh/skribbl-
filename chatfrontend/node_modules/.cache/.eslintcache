pher) => {\n    dispatch(process(encrypt, msg, cipher));\n  };\n\n  useEffect(() => {\n    socket.on(\"message\", (data) => {\n      //decypt the message\n      const ans = to_Decrypt(data.text, data.username);\n      dispatchProcess(false, ans, data.text);\n      console.log(ans);\n      let temp = messages;\n      temp.push({\n        userId: data.userId,\n        username: data.username,\n        text: ans,\n      });\n      setMessages([...temp]);\n    });\n  }, [socket]);\n\n  const sendData = () => {\n    if (text !== \"\") {\n      //encrypt the message here\n      const ans = to_Encrypt(text);\n      socket.emit(\"chat\", ans);\n      setText(\"\");\n    }\n  };\n  const messagesEndRef = useRef(null);\n\n  const scrollToBottom = () => {\n    messagesEndRef.current.scrollIntoView({ behavior: \"smooth\" });\n  };\n\n  useEffect(scrollToBottom, [messages]);\n\n  console.log(messages, \"mess\");\n\n  return (\n    <div className=\"chat\">\n      <div className=\"user-name\">\n        <h2>\n          {username} <span style={{ fontSize: \"0.7rem\" }}>in {roomname}</span>\n        </h2>\n      </div>\n      <div className=\"chat-message\">\n        {messages.map((i) => {\n          if (i.username === username) {\n            return (\n              <div className=\"message\">\n                <p>{i.text}</p>\n                <span>{i.username}</span>\n              </div>\n            );\n          } else {\n            return (\n              <div className=\"message mess-right\">\n                <p>{i.text} </p>\n                <span>{i.username}</span>\n              </div>\n            );\n          }\n        })}\n        <div ref={messagesEndRef} />\n      </div>\n      <div className=\"send\">\n        <input\n          placeholder=\"enter your message\"\n          value={text}\n          onChange={(e) => setText(e.target.value)}\n          onKeyPress={(e) => {\n            if (e.key === \"Enter\") {\n              sendData();\n            }\n          }}\n        ></input>\n        <button onClick={sendData}>Send</button>\n      </div>\n    </div>\n  );\n}\nexport default Chat;\n","/home/dhairya/Desktop/Scribble/chatfrontend/src/process/process.js",[],"/home/dhairya/Desktop/Scribble/chatfrontend/src/home/home.js",[],"/home/dhairya/Desktop/Scribble/chatfrontend/src/store/reducer/process.js",[],"/home/dhairya/Desktop/Scribble/chatfrontend/src/aes.js",[],"/home/dhairya/Desktop/Scribble/chatfrontend/src/store/action/index.js",[],"/home/dhairya/Desktop/sem3/Scribble/chatfrontend/src/index.js",[],["78","79"],"/home/dhairya/Desktop/sem3/Scribble/chatfrontend/src/App.js",[],"/home/dhairya/Desktop/sem3/Scribble/chatfrontend/src/store/reducer/index.js",[],"/home/dhairya/Desktop/sem3/Scribble/chatfrontend/src/process/process.js",["80"],"/home/dhairya/Desktop/sem3/Scribble/chatfrontend/src/home/home.js",[],"/home/dhairya/Desktop/sem3/Scribble/chatfrontend/src/chat/chat.js",["81"],"/home/dhairya/Desktop/sem3/Scribble/chatfrontend/src/store/reducer/process.js",[],"/home/dhairya/Desktop/sem3/Scribble/chatfrontend/src/aes.js",[],"/home/dhairya/Desktop/sem3/Scribble/chatfrontend/src/store/action/index.js",[],{"ruleId":"82","severity":1,"message":"83","line":31,"column":6,"nodeType":"84","endLine":31,"endColumn":14,"suggestions":"85"},{"ruleId":"86","replacedBy":"87"},{"ruleId":"88","replacedBy":"89"},{"ruleId":"82","severity":1,"message":"90","line":17,"column":5,"nodeType":"84","endLine":17,"endColumn":7,"suggestions":"91"},{"ruleId":"82","severity":1,"message":"83","line":31,"column":6,"nodeType":"84","endLine":31,"endColumn":14,"suggestions":"92"},"react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'dispatchProcess' and 'messages'. Either include them or remove the dependency array.","ArrayExpression",["93"],"no-native-reassign",["94"],"no-negated-in-lhs",["95"],"React Hook useEffect has missing dependencies: 'roomname' and 'socket'. Either include them or remove the dependency array.",["96"],["97"],{"de